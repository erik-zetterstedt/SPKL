{
  "title": "Caps key remaps",
  "author": "Erik",
  "homepage": "",
  "hostpage": "",
  "manual": "",
  "import_url": "",
  "rules": [
    {
      "description": "Caps ' -> `",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": ["right_command", "right_control", "right_shift", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "equal_sign",
              "modifiers": ["left_shift", "left_option"]
            }
          ]
        }
      ]
    },
    {
      "description": "Caps y -> backslash",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": ["right_command", "right_control", "right_shift", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": ["left_shift", "left_option"]
            }
          ]
        }
      ]
    },
    {
      "description": "Caps å -> |",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["right_command", "right_control", "right_shift", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": ["left_option"]
            }
          ]
        }
      ]
    },
    {
      "description": "Caps ö -> [",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": ["right_command", "right_control", "right_shift", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    },
    {
      "description": "Caps ä -> ]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "quote",
            "modifiers": {
              "mandatory": ["right_command", "right_control", "right_shift", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "equal_sign",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Caps { -> $",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": ["right_command", "right_control", "right_shift", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "4",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Caps f -> /",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["right_command", "right_control", "right_shift", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Caps r -> ^",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": ["right_command", "right_control", "right_shift", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": ["left_shift", "left_option"]
            }
          ]
        }
      ]
    }
  ]
}
